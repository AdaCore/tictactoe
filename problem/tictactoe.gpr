project Tictactoe is

   for Source_Dirs use ("src");
   for Object_Dir use "obj";
   for Main use ("game.adb");

   package Linker is
      for Switches ("ada") use ("-g");
   end Linker;

   package Compiler is
      for Switches ("ada") use ("-g");
   end Compiler;

   package Prove is
      for Switches use ("--warnings=off");
   end Prove;

end Tictactoe;

